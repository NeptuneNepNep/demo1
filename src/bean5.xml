<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
<!--实现自动装配 byName通过属性名称id值要和类中名称一样 byType根据属性类型-->
    <bean id="emp" class="sample.com.atguigu.autowrite.Emp" autowire="byName"></bean>
    <bean id="emp1" class="sample.com.atguigu.autowrite.Emp" autowire="byName">

</bean>
    <bean id="dept" class="sample.com.atguigu.autowrite.Dept"></bean>
</beans>